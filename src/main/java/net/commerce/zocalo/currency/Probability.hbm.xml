<hibernate-mapping package="net.commerce.zocalo.currency">
    <class name="Probability" table="PROBABILITY" mutable="false">
        <cache usage="read-only"/>
        <id name="id" column="Probability_ID">
            <generator class="increment"/>
        </id>
        <property name="quant"/>
    </class>
</hibernate-mapping>
